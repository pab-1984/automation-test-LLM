{
  "provider": "openai",
  "enabled": false,
  "apiKey": "env:OPENAI_API_KEY",
  "baseUrl": "https://api.openai.com/v1",
  "model": "gpt-4o-mini",
  "alternativeModels": [
    "gpt-4o",
    "gpt-4-turbo",
    "gpt-3.5-turbo"
  ],
  "temperature": 0.1,
  "maxTokens": 4096,
  "timeout": 60000,
  "retries": 2,
  "cost": {
    "inputPer1M": 0.15,
    "outputPer1M": 0.60,
    "currency": "USD"
  },
  "description": "OpenAI GPT - Alta calidad, requiere API key de pago"
}